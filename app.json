{
  "expo": {
    "name": "smartfit",
    "slug": "smartfit",
    "version": "1.0.0",
    "icon": "./assets/smartfit.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/smartfit.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "com.vaco.smartfit",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.vaco.smartfit",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/smartfit.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
